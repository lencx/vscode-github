{
  "name": "vscode-github",
  "displayName": "vscode-github",
  "description": "Explore Github repositories directly from Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "lencx",
  "author": "lencx <cxin1314@gmail.com>",
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p tsconfig.extension.json",
    "watch": "tsc -watch -p tsconfig.extension.json",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./build/test/runTest.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "homepage": "https://github.com/lencx/vscode-github",
  "repository": "git@github.com:lencx/vscode-github.git",
  "engines": {
    "vscode": "^1.50.0"
  },
  "bugs": {
    "url": "https://github.com/lencx/github/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-github.helloWorld",
    "onCommand:vscode-github.trending"
  ],
  "main": "./build/ext/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-github.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-github.trending",
        "title": "Open Github Trending",
        "category": "github:gt"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VS Code Github Trending Configuration",
      "properties": {
        "vscode-github.trending.languages": {
          "type": "array",
          "default": [
            "c++",
            "css",
            "go",
            "html",
            "javascript",
            "java",
            "php",
            "python",
            "ruby",
            "rust",
            "typescript"
          ],
          "description": "Names of the preferred languages"
        },
        "vscode-github.trending.selectedLanguage": {
          "type": "string",
          "default": "go",
          "description": "Selected language"
        },
        "vscode-github.trending.selectedInterval": {
          "type": "string",
          "enum": [
            "daily",
            "weekly",
            "monthly"
          ],
          "default": "monthly",
          "description": "Selected time interval (daily, weekly or monthly)"
        }
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
