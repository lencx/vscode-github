{
  "name": "vscode-github",
  "displayName": "VSGH",
  "description": "Explore Github repositories directly from Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "lencx",
  "author": "lencx <cxin1314@gmail.com>",
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "compile": "tsc -p tsconfig.extension.json",
    "watch": "tsc -watch -p tsconfig.extension.json",
    "pretest": "yarn run compile && yarn run lint",
    "start": "react-scripts start",
    "build": "./scripts/build-non-split.js && yarn run compile",
    "test": "node ./build/test/runTest.js",
    "lint": "eslint src --ext ts",
    "eject": "react-scripts eject"
  },
  "homepage": "https://github.com/lencx/vscode-github",
  "repository": "git@github.com:lencx/vscode-github.git",
  "engines": {
    "vscode": "^1.50.0"
  },
  "bugs": {
    "url": "https://github.com/lencx/github/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsgh.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsgh.start",
        "title": "Open Github Trending",
        "category": "vsgh"
      }
    ],
    "configuration": {}
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@octokit/core": "^3.2.4",
    "dayjs": "^1.9.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "husky": ">=4",
    "lint-staged": ">=10",
    "mocha": "^8.1.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1",
    "rewire": "^5.0.0",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,scss,css,md}": "prettier --write"
  }
}
